{
  "name": "nacme",
  "description": "Simple and unopinionated ACME client",
  "author": "nmorsman",
  "version": "2.3.3",
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/taoyuan/nacme",
  "dependencies": {
    "axios": "^0.18.0",
    "backo2": "^1.0.2",
    "bluebird": "^3.5.3",
    "debug": "^4.1.1",
    "fs-extra": "^7.0.1",
    "node-forge": "^0.8.1",
    "openssl-wrapper": "^0.3.4",
    "tempfile": "^2.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/chai": "latest",
    "@types/chai-as-promised": "^7.1.0",
    "@types/fs-extra": "^5.0.5",
    "@types/mocha": "latest",
    "@types/node": "latest",
    "@types/node-forge": "^0.7.11",
    "chai": "latest",
    "chai-as-promised": "^7.1.1",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "latest",
    "nock": "^10.0.6",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "docs": "jsdoc2md src/client.js > docs/client.md && jsdoc2md src/crypto/openssl.js > docs/openssl.md && jsdoc2md src/crypto/forge.js > docs/forge.md",
    "clean": "rimraf lib",
    "format": "prettier --write \"{src,test}/**/*.ts\" --single-quote --trailing-comma es5",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "npm run build && nyc mocha --require ts-node/register --recursive \"test/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taoyuan/nacme"
  },
  "keywords": [
    "simple",
    "acme",
    "acmev2",
    "client",
    "boulder",
    "lets",
    "encrypt"
  ],
  "bugs": {
    "url": "https://github.com/taoyuan/nacme/issues"
  }
}
